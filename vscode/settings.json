{
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["p", "p"],
      "after": ["p"]
    },
    {
      "before": ["p"],
      "after": ["\"", "_", "d", "P"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["extension.vim_ctrl+y"]
    },
    {
      "before": ["<S-Tab>"],
      "after": ["<", "<"]
    },
    {
      "before": ["<tab>"],
      "after": [">", ">"]
    },
    {
      "before": ["<leader>", "c", "<leader>"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<C-j>"],
      "after": ["i", "<CR>"]
    },
    {
      "before": ["C-a"],
      "after": ["g", "g", "V", "G"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "c", "<leader>"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c", "<leader>"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["C-a"],
      "after": ["g", "g", "V", "G"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["p"],
      "after": ["\"", "_", "d", "P"]
    },
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    }
  ],
  "git.autofetch": true,
  "editor.formatOnType": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 1,
  "files.associations": {
    ".env": "ini",
    "*.rmd": "markdown"
  },
  "vim.highlightedyank.enable": true,
  "vim.easymotion": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": true,
    "<C-v>": false,
    "<C-c>": false,
    "<C-y>": false,
    "<C-d>": false,
    "<C-k>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-w>": false
  },
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "editor.maxTokenizationLineLength": 20000000,
  "jest.autoEnable": false,
  "mocha.coverage": {
    "enable": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  // "workbench.colorCustomizations": {
  //   "foreground": "#c0c0c0",
  //   "textPreformat.foreground": "#c0c0c0",
  //   "editor.foreground": "#c0c0c0",
  //   "editorLineNumber.foreground": "#c0c0c0"
  // },
  "editor.multiCursorModifier": "ctrlCmd",
  "typescript.format.insertSpaceAfterConstructor": true,
  "workbench.sideBar.location": "left",
  "vim.whichwrap": "[,],<,>",
  "plantuml.render": "PlantUMLServer",
  "plantuml.server": "http://localhost:8001/",
  "vim.normalModeKeyBindings": [],
  "debug.openDebug": "openOnDebugBreak",
  "aws.samcli.location": "/home/linuxbrew/.linuxbrew/bin/sam",
  "aws.profile": "profile:codehashi",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "todo-tree.tree.showScanModeButton": false,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "workbench.startupEditor": "newUntitledFile",
  "editor.formatOnSave": true,
  "debug.javascript.autoAttachFilter": "smart",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false
}
